{
  "release3-orgunit": {
    "mappings": {
      "properties": {
        "acronym": {
          "type": "keyword"
        },
        "city": {
          "type": "keyword"
        },
        "coordinates": {
          "type": "geo_point"
        },
        "country": {
          "type": "keyword"
        },
        "id": {
          "type": "keyword"
        },
        "name": {
          "type": "text"
        },
        "name_completion": {
          "type": "completion",
          "analyzer": "simple",
          "preserve_separators": true,
          "preserve_position_increments": true,
          "max_input_length": 50
        },
        "name_keyword": {
          "type": "keyword"
        },
        "name_suggest": {
          "type": "search_as_you_type",
          "doc_values": false,
          "max_shingle_size": 3
        },
        "state": {
          "type": "keyword"
        }
      }
    }
  }
}
